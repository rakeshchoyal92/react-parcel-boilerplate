{
  "presets": [
     "@babel/preset-env",
     "@babel/preset-react"
   ],
  "plugins": [
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@actions": "./src/store/actions",
        "@src": "./src"
      }
    }]
  ]
}
